doctype 5
html(lang="en")
  head
    title #{title}
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    link(href="#{baseDomain}/libcss/bootstrap.css", rel="stylesheet")
    block stylesheet
    link(href="#{baseDomain}/libcss/bootstrap-responsive.css", rel="stylesheet")
  body
    include navbar
    block content
    script(src='#{baseDomain}/libjs/jquery-1.8.2.min.js')
    script(src='#{baseDomain}/libjs/bootstrap-collapse.js')
    block script
    include footer
